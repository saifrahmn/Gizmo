---
/*! feather-icons | MIT License | https://feathericons.com/ */

export { Props } from "./Props.ts";

let {
	size,
	title,
	width = size,
	height = size,
	...props
} = {
	"stroke": "CurrentColor",
	"fill": "none",
	"size": "24",
	"stroke-linecap": "round",
	"stroke-linejoin": "round",
	"stroke-width": "2",
	"title": "Octagon",
	"viewBox": "0 0 24 24",
	...Astro.props
}

const toAttributeSize = (size: number) => String(size).replace(/(?<=[0-9])(x%)$/, "em")

size = toAttributeSize(size)
width = toAttributeSize(width)
height = toAttributeSize(height)
---
<svg {width} {height} {...props}>{title ? (<title>{title}</title>) : ""}<polygon points="7.86 2 16.14 2 22 7.86 22 16.14 16.14 22 7.86 22 2 16.14 2 7.86 7.86 2"></polygon></svg>