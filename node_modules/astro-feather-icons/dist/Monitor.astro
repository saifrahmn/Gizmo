---
/*! feather-icons | MIT License | https://feathericons.com/ */

export { Props } from "./Props.ts";

let {
	size,
	title,
	width = size,
	height = size,
	...props
} = {
	"stroke": "CurrentColor",
	"fill": "none",
	"size": "24",
	"stroke-linecap": "round",
	"stroke-linejoin": "round",
	"stroke-width": "2",
	"title": "Monitor",
	"viewBox": "0 0 24 24",
	...Astro.props
}

const toAttributeSize = (size: number) => String(size).replace(/(?<=[0-9])(x%)$/, "em")

size = toAttributeSize(size)
width = toAttributeSize(width)
height = toAttributeSize(height)
---
<svg {width} {height} {...props}>{title ? (<title>{title}</title>) : ""}<rect x="2" y="3" width="20" height="14" rx="2" ry="2"></rect><line x1="8" y1="21" x2="16" y2="21"></line><line x1="12" y1="17" x2="12" y2="21"></line></svg>