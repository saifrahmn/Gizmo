---
/*! feather-icons | MIT License | https://feathericons.com/ */

export { Props } from "./Props.ts";

let {
	size,
	title,
	width = size,
	height = size,
	...props
} = {
	"stroke": "CurrentColor",
	"fill": "none",
	"size": "24",
	"stroke-linecap": "round",
	"stroke-linejoin": "round",
	"stroke-width": "2",
	"title": "Archive",
	"viewBox": "0 0 24 24",
	...Astro.props
}

const toAttributeSize = (size: number) => String(size).replace(/(?<=[0-9])(x%)$/, "em")

size = toAttributeSize(size)
width = toAttributeSize(width)
height = toAttributeSize(height)
---
<svg {width} {height} {...props}>{title ? (<title>{title}</title>) : ""}<polyline points="21 8 21 21 3 21 3 8"></polyline><rect x="1" y="3" width="22" height="5"></rect><line x1="10" y1="12" x2="14" y2="12"></line></svg>