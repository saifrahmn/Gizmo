---
/*! feather-icons | MIT License | https://feathericons.com/ */

export { Props } from "./Props.ts";

let {
	size,
	title,
	width = size,
	height = size,
	...props
} = {
	"stroke": "CurrentColor",
	"fill": "none",
	"size": "24",
	"stroke-linecap": "round",
	"stroke-linejoin": "round",
	"stroke-width": "2",
	"title": "Truck",
	"viewBox": "0 0 24 24",
	...Astro.props
}

const toAttributeSize = (size: number) => String(size).replace(/(?<=[0-9])(x%)$/, "em")

size = toAttributeSize(size)
width = toAttributeSize(width)
height = toAttributeSize(height)
---
<svg {width} {height} {...props}>{title ? (<title>{title}</title>) : ""}<rect x="1" y="3" width="15" height="13"></rect><polygon points="16 8 20 8 23 11 23 16 16 16 16 8"></polygon><circle cx="5.5" cy="18.5" r="2.5"></circle><circle cx="18.5" cy="18.5" r="2.5"></circle></svg>