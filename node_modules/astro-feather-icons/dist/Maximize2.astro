---
/*! feather-icons | MIT License | https://feathericons.com/ */

export { Props } from "./Props.ts";

let {
	size,
	title,
	width = size,
	height = size,
	...props
} = {
	"stroke": "CurrentColor",
	"fill": "none",
	"size": "24",
	"stroke-linecap": "round",
	"stroke-linejoin": "round",
	"stroke-width": "2",
	"title": "Maximize 2",
	"viewBox": "0 0 24 24",
	...Astro.props
}

const toAttributeSize = (size: number) => String(size).replace(/(?<=[0-9])(x%)$/, "em")

size = toAttributeSize(size)
width = toAttributeSize(width)
height = toAttributeSize(height)
---
<svg {width} {height} {...props}>{title ? (<title>{title}</title>) : ""}<polyline points="15 3 21 3 21 9"></polyline><polyline points="9 21 3 21 3 15"></polyline><line x1="21" y1="3" x2="14" y2="10"></line><line x1="3" y1="21" x2="10" y2="14"></line></svg>